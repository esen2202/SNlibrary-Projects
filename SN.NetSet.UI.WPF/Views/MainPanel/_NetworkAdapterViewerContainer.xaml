<UserControl x:Class="SN.NetSet.UI.WPF.Views.MainPanel._NetworkAdapterViewerContainer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SN.NetSet.UI.WPF.Views.MainPanel"
             xmlns:networkAdapterViewer="clr-namespace:SN.NetSet.UI.WPF.Views.UserControls.NetworkAdapterViewer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="320">
    <Grid DataContext="{Binding CurrentAdapter}">
        <networkAdapterViewer:_NetworkAdapterViewer />
    </Grid>
</UserControl>
